{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "es6",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"noUnusedLocals": false,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.js",
		"**/*.mjs",
		"**/*.ts",
		"**/*.tsx",
		"**/*.json",
		"*.json",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules", "package-lock.json", "out/", ".next"]
}
